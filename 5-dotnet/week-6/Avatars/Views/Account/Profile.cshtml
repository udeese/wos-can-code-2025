@model ProfilePageViewModel
@{
    ViewData["Title"] = "Account Profile";
    var profile = Model.ProfileViewModel;
    var profileForm = Model.ProfileFormViewModel;
}

<h1 class="display-4">Profile</h1>

<div class="row">
    <div class="col">
        @await Html.PartialAsync("_ProfileCard", profile)
    </div>
    <div class="col">
        @await Html.PartialAsync("_ProfileForm", profileForm)
    </div>
</div>